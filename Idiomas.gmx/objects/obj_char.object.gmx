        <arguments>
          <argument>
            <kind>1</kind>
            <string>if(playable){
    var obj;
    switch(position){
        case 0:
            obj = collision_rectangle(x, y + 30, x + 64, y + 49, obj_char, false, false)
            break;
        case 1:
            obj = collision_rectangle(x + 64, y, x + 84, y + 64, obj_char, false, false)
            break;
        case 2:
            obj = collision_rectangle(x, y + 64, x + 64, y + 84, obj_char, false, false)
            break;
        case 3:
            obj = collision_rectangle(x - 20, y, x, y + 64, obj_char, false, false)
            break;
    }
    
    if(obj != noone){
        if(arraysCanTalkTo(languages, obj.languages)){
            if(obj.receiver){
                instance_create(window_get_width()/2 - sprite_get_width(spr_next)/2, window_get_height() - sprite_get_height(spr_next) - 20, obj_win)
                obj_win.next_level_room = obj_controller.next_level_room
            }
            name = ""
            playable = false;
            objToChange = obj
            if(!obj.receiver){
                obj.name = getLanguageArraySign(obj.languages)
            }
            name = getLanguageArraySign(languages)
            
            alarm[0] = 1
        } else {
            obj_controller.showLanguages = true
            var msg;
            if(array_length_1d(obj.languages) == 1){
                msg = getLanguageName(obj.languages[0])
            } else {
                msg = getLanguageName(obj.languages[0]) + " e " + getLanguageName(obj.languages[1])
            }
            obj_controller.message = "Comunicação impossível, o personagem sabe apenas " + msg;
            obj_controller.alarm[0] = 120
            
            obj.name = getLanguageArraySign(obj.languages)
 
        }
    } 
}

