            <string>depth = -y

var walk_speed = 3;

if(!playable){
    if(locked){
    
        if(x &lt; 15) {
            walk_x_mov_speed = walk_speed;
        } else if(x &gt; window_get_width() - 71){
            walk_x_mov_speed = -walk_speed;
        } else if(y &lt; 35){
            walk_y_mov_speed = walk_speed;
        } else if(y &gt; window_get_height() - 90){
            walk_y_mov_speed = -walk_speed;
        }
        
        x += walk_x_mov_speed
        y += walk_y_mov_speed
        
        if(walk_x_mov_speed &gt; 0){
            sprite_index = sprs[spr, 4]
        } else if(walk_x_mov_speed &lt; 0){
            sprite_index = sprs[spr, 6]
        } else if(walk_y_mov_speed &gt; 0){
            sprite_index = sprs[spr, 0]
        } else if(walk_y_mov_speed &lt; 0){
            sprite_index = sprs[spr, 2]
        }
        
    } else {
        var dir = irandom_range(0, 3);
        
        switch(dir){
            case 0:
                walk_x_mov_speed = 0;
                walk_y_mov_speed = -3;
                break;
            case 1:
                walk_x_mov_speed = 3;
                walk_y_mov_speed = 0;
                break;
            case 2:
                walk_x_mov_speed = 0;
                walk_y_mov_speed = -3;
                break;
            case 3:
                walk_x_mov_speed = -3;
                walk_y_mov_speed = 0;
                break;
        }
        locked = true;
        alarm[1] = irandom_range(30, 75);
    }
} else {

    if(x &lt; 15) {
        x = 15;
    } else if(x &gt; window_get_width() - 71){
        x = window_get_width() - 71;
    }
    
    if(y &lt; 35){
        y = 35;
    } else if(y &gt; window_get_height() - 90){
        y = window_get_height() - 90;
    }
}
